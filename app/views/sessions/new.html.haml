%h1 Log in
= form_with(scope: :session, url: login_path) do |f|
  .form-group
    = f.label :user_name
    %br/
    = f.text_field :user_name, class: "form-control"
  .form-group
    = f.label :password
    %br/
    = f.password_field :password, class: "form-control"
  .form-group
    = f.submit "Log in", class: "btn btn-primary"
